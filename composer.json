{
    "name": "paulr/php-framework",
    "description": "Basic custom PHP framework to build websites",
    "authors": [
        {
            "name": "Paul Rodriguez"
        }
    ],
    "require": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
    "psr-4": {
      "Model\\":"app/models/",
      "Config\\":"app/config/",
      "Routes\\":"app/routes/",
      "Controller\\":"app/controllers/"
    },
    "autoload-dev": {
    	"psr-4": {
            "Tests\\": "tests/",
            "Tests\\Unit\\":"tests/Unit"
        }
    }
  }
}
